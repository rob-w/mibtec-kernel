		reg = <0x4c>;
		pinctrl-names = "default";
		pinctrl-0 = <&tlv320adc_pins>;
		ti,mic-bias-source = <6>;
		ti,vref-source = <0>;
		ti,pdm-edge-select = <0 1 0 1>;
		ti,gpi-config = <2 1 1 1>;
		ti,gpio-config = <10 2>;
		ti,gpo-config-1 = <0 0>;
		ti,gpo-config-2 = <0 0>;
		reset-gpios = <&gpio5 8 GPIO_ACTIVE_HIGH>;
	};

	tmp102: tmp102@48 {
		compatible = "ti,tmp102";
		reg = <0x48>;
		#thermal-sensor-cells = <1>;
	};
};

