			label = "misdimm-evm_usr1";
			gpios = <&gpio5 8 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "mmc0";
		};*/

		/* 0x23c P600_142 F24 gpio5_9.gpio5_9 */
