			0x04c (PIN_INPUT_PULLDOWN | MUX_MODE7)			/* P600_159 A4 gpmc_a3.gpio1_19 */
			0x050 (PIN_INPUT_PULLDOWN | MUX_MODE7)			/* P600_161 D7 gpmc_a4.gpio1_20 */
