			0x0d4 (PIN_INPUT_PULLDOWN | MUX_MODE7) /* P600_90  D19 dss_data13.gpio0_9 */
			0x0d8 (PIN_INPUT_PULLDOWN | MUX_MODE7) /* P600_91  C17 dss_data14.gpio0_10 */
