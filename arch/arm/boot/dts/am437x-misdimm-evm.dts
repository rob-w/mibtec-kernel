			0x130 (PIN_INPUT_PULLDOWN | MUX_MODE7)			/* P600_107 D13 mii1_rxclk.gpio3_10 */
			0x134 (PIN_INPUT | PULL_DISABLE | MUX_MODE7)	/* P600_117 C14 mii1_rxd3.gpio2_18 */
