			0x114 (PIN_INPUT_PULLDOWN | MUX_MODE7)			/* P600_119 A13 mii1_txen.gpio3_3 */
			0x118 (PIN_INPUT_PULLDOWN | MUX_MODE7)			/* P600_109 A15 mii1_rxdv.gpio3_4 */
